{
    "name": "sergeil/extjsintegration-bundle",
    "description": "Utilities for easier integration of ExtJs on server-side.",
    "keywords": ["extjs"],
    "homepage": "http://github.com/sergeil/SliExtJsIntegrationBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@gmail.com",
            "homepage": "http://sergei.lissovski.org/"
        }
    ],
    "conflict": {
        "doctrine/persistence": "<1.3"
    },
    "require": {
        "php": ">=7.1",
        "sergeil/aux-bundle": "^1.1",
        "sergeil/expander-bundle": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "doctrine/doctrine-bundle": "^2.0",
        "symfony/symfony": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Sli\\ExtJsIntegrationBundle\\": ""
        },
        "classmap": [ "DataMapping/Annotations.php", "Service/SortingFieldAnnotations.php" ],
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sli\\ExtJsIntegrationBundle\\Tests\\": "Tests"
        },
        "files": [
            "Tests/Fixtures/App/app/AppKernel.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}